@startuml

left to right direction

rectangle Registrodeasistencias {
  
  actor alumno
  actor profesor
  usecase "Verificar Tarjeta" as UC1
  usecase "Lectura de tarjeta [002]" as UC2
  usecase "Alta de alumno [003]" as UC3
  usecase "Modificar alumno [005]" as UC5
  alumno -- UC1
  UC1 ..> UC2 : include
  UC1 ..> UC5 : extend
  UC1 ..> UC3 : extend
  profesor -- UC2
  profesor -- UC3
  profesor -- UC5
}

@enduml
