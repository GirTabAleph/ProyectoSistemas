@startuml


rectangle ReporteDeInformes {

  actor Profesor
  actor "Sistema De Asistencia" as sistema

  usecase "Seleccione Informe de Asistencias" as UC1
  usecase "Verificar Fecha" as UC2
  usecase "Seleccionar Grupo" as UC3
  usecase "Mostrar Lista De Asistencia" as UC4
  usecase "Exportar Informes" as UC5

  Profesor -- UC1
  UC1 --> sistema
  sistema -- UC2
  UC2 -- UC3
  Profesor -- UC3
  UC3 -- UC4
  UC4 -- UC5


}
@enduml
