@startuml


rectangle RegistrodeAlumno {

  actor Profesor

  actor "AplicaciÃ³n de asistencias" as app
  actor "Administracion Escolar" as app1

  usecase "Registrar Alumno" as UC3
  usecase "Datos del alumno" as UC4
  usecase "Solicitar Grupo y Tarjeta" as UC5

  app -- UC5
